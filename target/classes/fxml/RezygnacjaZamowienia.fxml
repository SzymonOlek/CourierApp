<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="resignOrderBackground" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" stylesheets="@rezygnacja.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Kurier.klientControler">
   <children>
      <Button fx:id="resignOrder" mnemonicParsing="false" onAction="#onActionResignOrder" text="Rezygnuj">
         <StackPane.margin>
            <Insets top="50.0" />
         </StackPane.margin>
      </Button>
      <TextField fx:id="resignOrderText" alignment="CENTER" maxWidth="350.0" promptText="Podaj numer zamowienia z którego chcesz zrezygnowac">
         <StackPane.margin>
            <Insets bottom="100.0" />
         </StackPane.margin>
      </TextField>
      <Button fx:id="backResignOrder" mnemonicParsing="false" onAction="#onActionBackResignOrder" text="Powrót">
         <StackPane.margin>
            <Insets left="350.0" top="250.0" />
         </StackPane.margin>
      </Button>
   </children>
</StackPane>
